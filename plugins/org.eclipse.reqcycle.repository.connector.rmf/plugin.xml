<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.reqcycle.repository.connector.connectorCore">
      <connectorCore
            class="org.eclipse.reqcycle.repository.connector.rmf.RMFConnector"
            id="org.eclipse.reqcycle.repository.connector.rmf.connectorCore"
            name="ReqIf">
      </connectorCore>
   </extension>
   <extension
         point="org.eclipse.reqcycle.repository.connector.ui.connectorUi">
      <connectorUi
            class="org.eclipse.reqcycle.repository.connector.rmf.RMFConnectorUi"
            icon="icons/RMF.png"
            id="org.eclipse.reqcycle.repository.connector.rmf.connectorCore">
      </connectorUi>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.eclipse.reqcycle.repository.connector.rmf.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="org.eclipse.reqcycle.repository.connector.rmf.commands.category"
            id="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand"
            name="Sample Command">
      </command>
      <category
            id="org.eclipse.reqcycle.repository.connector.rmf.commands.category2"
            name="Sample Category 2">
      </category>
      <command
            categoryId="org.eclipse.reqcycle.repository.connector.rmf.commands.category2"
            id="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand2"
            name="Sample Command 2">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.reqcycle.repository.connector.rmf.handlers.SampleHandler"
            commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand">
      </handler>
      <handler
            class="org.eclipse.reqcycle.repository.connector.rmf.handlers.SampleHandler2"
            commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand2">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
      <key
            commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand2"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.eclipse.reqcycle.repository.connector.rmf.menus.sampleMenu"
               label="Sample Menu"
               mnemonic="M">
            <command
                  commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand"
                  id="org.eclipse.reqcycle.repository.connector.rmf.menus.sampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.reqcycle.repository.connector.rmf.toolbars.sampleToolbar">
            <command
                  commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand"
                  icon="icons/sample.gif"
                  id="org.eclipse.reqcycle.repository.connector.rmf.toolbars.sampleCommand"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.eclipse.reqcycle.repository.connector.rmf.menus.sampleMenu2"
               label="Sample Menu 2"
               mnemonic="M">
            <command
                  commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand2"
                  id="org.eclipse.reqcycle.repository.connector.rmf.menus.sampleCommand2"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.reqcycle.repository.connector.rmf.toolbars.sampleToolbar2">
            <command
                  commandId="org.eclipse.reqcycle.repository.connector.rmf.commands.sampleCommand2"
                  icon="icons/sample.gif"
                  id="org.eclipse.reqcycle.repository.connector.rmf.toolbars.sampleCommand2"
                  tooltip="Say hello world 2">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
</plugin>
